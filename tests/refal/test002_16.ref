$EXTERN Lexical;
$EXTERN Syntax;
$EXTERN DirectivesSyntax;
$EXTERN Linker;
$EXTERN Pragma;

$ENTRY Start {
  t.File e.Files = <Linker <DirectivesProcess t.File> <Process e.Files>>;
  e.Foo = <Prout 'Bug in "Start" function. View field: ' e.Foo>;
}

Process {
  t.File e.Files = <Syntax <Lexical t.File>> <Process e.Files>;
    = ;
}

DirectivesProcess {
  t.File = <DirectivesSyntax <Lexical <Pragma t.File>>>;
  e.Foo = <Prout 'Bug in "DirectivesProcess" function. View field: ' e.Foo>;
}
